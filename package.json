{
  "name": "@lehmax/datatable",
  "author": "Maxime Leherquer",
  "version": "1.0.0-alpha.20",
  "description": "A simple datatable for React",
  "keywords": [
    "react",
    "datatable",
    "table"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lehmax/DataTable"
  },
  "type": "module",
  "license": "MIT",
  "module": "dist/index.es.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
